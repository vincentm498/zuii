{
	"name": "zuii",
	"version": "1.4.0",
	"description": "Bibliothèque de composants UI légère, intuitive et modulaire pour les interfaces web modernes.",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vincentm498/zuii.git"
	},
	"bugs": {
		"url": "https://github.com/vincentm498/zuii/issues"
	},
	"homepage": "https://github.com/vincentm498/zuii#readme",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"scripts/tokens"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./templates": {
			"import": "./dist/templates/index.ts",
			"types": "./dist/templates/index.d.ts"
		},
		"./core/*": "./dist/core/*"
	},
	"bin": {
		"zuii-tokens": "./scripts/tokens/build.js"
	},
	"scripts": {
		"dev": "vite",
		"prebuild": "pnpm run tokens:build",
		"build": "vite build",
		"postbuild": "mkdir -p dist/core/styles && cp src/core/styles/_tokens.scss src/core/styles/tokens.css src/core/styles/tokens.ts dist/core/styles/ && cp -r src/templates dist/",
		"preview": "vite preview",
		"prepublishOnly": "pnpm run build",
		"tokens:build": "node scripts/tokens/build.js --output src/core/styles",
		"component:create": "node scripts/create-component.js"
	},
	"keywords": [],
	"author": "Vincent Moreau",
	"license": "MIT",
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"@ag-grid-community/locale": "^35.0.1",
		"@uppy/core": "^5.2.0",
		"@uppy/dashboard": "^5.1.1",
		"@uppy/locales": "^5.1.1",
		"@uppy/react": "^5.2.0",
		"ag-grid-community": "^35.0.1",
		"ag-grid-react": "^35.0.1",
		"apca-w3": "^0.1.9",
		"bootstrap": "^5.3.8",
		"choices.js": "^11.1.0",
		"colord": "^2.9.3",
		"flag-icons": "^7.5.0",
		"intl-tel-input": "^26.1.1",
		"lucide-static": "^0.563.0",
		"react-bootstrap": "^2.10.10",
		"sass-embedded": "^1.97.3",
		"style-dictionary": "^4.2.0",
		"sweetalert2": "^11.26.18",
		"sweetalert2-react-content": "^5.1.1"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.3",
		"@types/intl-tel-input": "^18.1.4",
		"@types/node": "^25.1.0",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"sass": "^1.97.3",
		"semantic-release": "^25.0.2",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-lib-inject-css": "^2.2.2"
	}
}
